{
  "name": "postpress",
  "version": "1.0.0",
  "description": "1. [Database](./docs/database.md) 2. Routes    1. [User](./docs/user-routes.md)",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "dev:run": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint 'src/**' --quiet",
    "lint:fix": "eslint 'src/**' --quiet --fix",
    "lint:staged": "lint-staged",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests --runInBand",
    "test:unit": "npm run test -- --watch -c ./jest-unit.config.js ",
    "test:docker:up": "docker-compose --env-file ./.env.test -f ./tests/docker-compose.yml up -d",
    "test:docker:down": "docker-compose -f ./tests/docker-compose.yml down",
    "test:prepare:integration": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test:integration": "npm run test:prepare:integration && dotenv -e .env.test -- npm run test -- -c ./jest-integration.config.js",
    "test:ci": "npx prisma migrate deploy && npm run test -- --coverage",
    "test:ci:local": "dotenv -e .env.test -- npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmviniciuss/postpress.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bmviniciuss/postpress/issues"
  },
  "homepage": "https://github.com/bmviniciuss/postpress#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "dotenv-cli": "^4.1.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "factory-maker": "^0.2.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^4.0.7",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^12.3.1",
    "prisma": "^3.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "supertest": "^6.2.2",
    "swagger-ui-express": "^4.3.0"
  }
}
